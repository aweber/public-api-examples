	"item": [
		{
			"name": "Get Account",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "74e7dce1-167a-4675-8431-bdf2223fcd25",
						"exec": [
							"pm.test(\"Successfully get an account Id\", function() {",
							"    pm.response.to.have.status(200);",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.entries[0].id).to.be.a('number')",
							"    pm.globals.set(\"accountId\",jsonData.entries[0].id )",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
					"raw": "https://api.aweber.com/1.0/accounts",
					"protocol": "https",
					"host": [
						"api",
						"aweber",
						"com"
					],
					"path": [
						"1.0",
						"accounts"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Lists",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "f6163948-14a0-48e5-ad18-ee37ab1a3c0d",
						"exec": [
							"pm.test(\"Successfully get a  random list\", function() {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"At least two lists found\", function() {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.total_size).to.be.above(1);",
							"    pm.globals.set(\"source_listId\",jsonData.entries[0].id );",
							"    pm.globals.set(\"destination_listId\",jsonData.entries[1].id );",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
					"raw": "https://api.aweber.com/1.0/accounts/{{accountId}}/lists",
					"protocol": "https",
					"host": [
						"api",
						"aweber",
						"com"
					],
					"path": [
						"1.0",
						"accounts",
						"{{accountId}}",
						"lists"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Subscribers on Source List",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "75df1ad8-7a66-40eb-b8e7-2991fcd51619",
						"exec": [
							"pm.test(\"Successfully get a list\", function(){",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"List has at least one subscriber\", function() {",
							"   var jsonData = pm.response.json() ;",
							"   pm.expect(jsonData.total_size).to.be.above(0);",
							"   pm.globals.set(\"subscriber_link\", jsonData.entries[0].self_link);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
					"raw": "{{base_URL}}/accounts/{{accountId}}/lists/{{source_listId}}/subscribers",
					"host": [
						"{{base_URL}}"
					],
					"path": [
						"accounts",
						"{{accountId}}",
						"lists",
						"{{source_listId}}",
						"subscribers"
					]
				}
			},
			"response": []
		},
