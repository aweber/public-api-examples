		{
			"name": "Move subscriber",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "054015e7-1062-4de8-9dc2-8e4b84849b25",
						"exec": [
							"pm.test(\"Successfully moved subscriber\", function(){",
							"    pm.response.to.have.status(201);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/x-www-form-urlencoded",
						"type": "text"
					}
				],
				"body": {
					"mode": "urlencoded",
					"urlencoded": [
						{
							"key": "list_link",
							"value": "{{destination_listId}}",
							"type": "text"
						},
						{
							"key": "ws.op",
							"value": "move",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{subscriber_link}}",
					"host": [
						"{{subscriber_link}}"
					]
				}
			},
			"response": []
		}
