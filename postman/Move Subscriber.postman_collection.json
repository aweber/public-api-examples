			"name": "Get Subscriber",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "8e652d7e-fd45-4591-a567-b0c9589d9ce6",
						"exec": [
							"pm.test(\"Successfully requested subscriber info\", function(){",
							"    pm.response.to.have.status(200);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
