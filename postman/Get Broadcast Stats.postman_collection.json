						"exec": [
							"pm.test(\"Successfully retreived sent broadcasts from list\", function() {",
							"  var jsonData = pm.response.json();",
							"  pm.globals.set(\"broadcast_self_link\", '');",
							"  if (pm.expect(jsonData.entries.length).to.be.above(0)) {",
							"    var broadcastNumber = Math.floor((Math.random() * jsonData.entries.length ));",
							"    pm.globals.set(\"broadcast_self_link\", jsonData.entries[broadcastNumber].self_link );",
